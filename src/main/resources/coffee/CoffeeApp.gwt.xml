<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="coffee">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.user.theme.clean.Clean"/>
    <inherits name="dagger.Dagger"/>
    <inherits name="com.google.common.util.concurrent.Concurrent"/>
    <inherits name="rx.Rx"/>

    <entry-point class="coffee.client.CoffeeApp"/>

    <define-configuration-property name="project.version" is-multi-valued="false"/>

    <collapse-all-properties/>

    <source path="client"/>
    <public path="public"/>
</module>
